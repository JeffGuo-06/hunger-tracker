{
    "family": "muckd-web",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "2048",
    "executionRoleArn": "arn:aws:iam::656362079087:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::656362079087:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "muckd-web",
            "image": "656362079087.dkr.ecr.us-east-2.amazonaws.com/muckd:latest-amd64",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "DJANGO_SETTINGS_MODULE",
                    "value": "muckd.settings.production"
                },
                {
                    "name": "AWS_STORAGE_BUCKET_NAME",
                    "value": "muckd-media"
                },
                { "name": "SECRET_KEY", "value": "django-insecure-+7#mvf$@#g=zi^v(2n0+z6!4mz-xuscxise1m=nw24-&6q+p@^" },
                { "name": "DB_NAME", "value": "muckd-db" },
                { "name": "DB_USER", "value": "postgres" },
                { "name": "DB_PASSWORD", "value": "Hudson1967%21%24" },
                { "name": "DB_HOST", "value": "muckd-db.cd0iyg2q8wyj.us-east-2.rds.amazonaws.com" },
                { "name": "DB_PORT", "value": "5432" },
                { "name": "REDIS_URL", "value": "redis://muckd-redis.4ikzba.0001.use2.cache.amazonaws.com:6379/0" },
                { "name": "AWS_ACCESS_KEY_ID", "value": "AKIAZRUR2QNX54ETVJUT" },
                { "name": "AWS_SECRET_ACCESS_KEY", "value": "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" },
                { "name": "PYTHONUNBUFFERED", "value": "1" }
            ],
            "healthCheck": {
                "command": ["CMD-SHELL", "curl -f http://localhost:80/health/ || exit 1"],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 120
            },
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/muckd-web",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "mountPoints": [
                {
                    "sourceVolume": "app-storage",
                    "containerPath": "/app/staticfiles",
                    "readOnly": false
                },
                {
                    "sourceVolume": "app-storage",
                    "containerPath": "/app/media",
                    "readOnly": false
                }
            ]
        }
    ],
    "volumes": [
        {
            "name": "app-storage",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-01df83f135f4aef25",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED"
            }
        }
    ]
} 


